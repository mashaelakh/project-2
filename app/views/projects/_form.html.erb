<%= form_with(model: project, local: true) do |form| %>
    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>

    <div class="field">
      <%= form.label :description %>
      <%= form.text_field :description %>
    </div>

    <div class="field">
      <%= form.label :contactinfo %>
      <%= form.text_field :contactinfo %>
    </div>

  
    <div class="actions">
      <%= form.submit %>
    </div>
<% end %>
