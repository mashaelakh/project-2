<div class= "center">
<div class= "new">
<h1>Edit Project</h1>
<%= form_for @project, url: {action: "update"} do |form|%>
<%= form.label :name%>
    <br>
    <%= form.text_field :name%>
  <br>
  <br>
    <%= form.label :description%>
    <br>
    <%=form.text_area :description , size: "50x 10"%>
<br>
    <%= form.label :contactinfo%>
    <br>
    <%= form.text_field :contactinfo %>
<br>
<br>
<%= form.submit "edit" , :class=>"btn btn-dark"%>
<%end%>
<br>
<%= link_to 'Back', projects_path , :class=>"btn btn-dark"%>
</div> </div>